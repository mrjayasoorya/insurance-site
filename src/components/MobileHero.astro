---
const { brand } = Astro.props;
---
<div class="m-hero">
  <h1>{brand.name}</h1>
  <p>{brand.subtitle}</p>
  <div class="signup" style="justify-content:center;">
    <a class="btn" href={`tel:${brand.phone}`} aria-label={`Call Now ${brand.phone}`}>
      Call Now {brand.phone}
    </a>
  </div>
</div>
